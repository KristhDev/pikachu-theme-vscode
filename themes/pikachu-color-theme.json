	"tokenColors": [
		/* MAIN Colors -------------------------------------------------------------- */
		{
			"name": "[COMMENTS] — The main comments color",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#4f6880"
			}
		},
		{
			"name": "[Entity] — The main Entity color",
			"scope": "entity",
			"settings": {
				"foreground": "#EC2864"
			}
		},
		{
			"name": "[Variable] - Variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#dfeeff"
			}
		},
		{
			"name": "[Variable Language] - Color for Variable Language (.this)",
			"scope": "variable.language",
			"settings": {
				"foreground": "#ffa4bd"
			}
		},
		{
			"name": "[Storage] — The main color for Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#FFB86C"
			}
		},
		{
			"name": "[Keyword] — The main color for Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#ff659d"
			}
		},
		{
			"name": "[String] - The STRING colors",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#35FFDC"
			}
		},
		// {
		// 	"name": "[Meta] — The main color for Meta",
		// 	"scope": "meta",
		// 	"settings": {
		// 		"foreground": "#FAD000"
		// 	}
		// },
		{
