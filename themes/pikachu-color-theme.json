			"name": "[Object keys] - Object keys",
			"scope": [
				"meta.object-literal.key",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#fff1ad"
			}
		},
		{
			"name": "[Keyword Operator] — Color for : / + = !==",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffadcb"
